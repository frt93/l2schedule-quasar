{
  "name": "first",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "l2Schedule2",
  "cordovaId": "org.cordova.quasar.app",
  "author": "frt <ectoprime@gmail.com>",
  "private": true,
  "scripts": {
    "app": "quasar dev --mode ssr",
    "api": "nodemon -w api api/server.js",
    "dev": "concurrently --kill-others \"npm run app\" \"npm run api\"",
    "docker": "docker-compose up -d",
    "build": "quasar build --mode ssr",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "postinstall": "link-module-alias",
    "db-backup": "docker-compose exec postgres pg_dumpall -c -U postgres > dump.sql",
    "db-restore": "docker-compose exec postgres psql -f dump.sql"
  },
  "dependencies": {
    "@quasar/extras": "^1.1.2",
    "ajv": "4.11.8",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "browser-cookies": "^1.2.0",
    "cookie": "^0.3.1",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "express-locale": "^1.2.0",
    "express-request-language": "^1.1.15",
    "graphql-request": "^1.8.2",
    "ioredis": "^4.9.5",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.11.0",
    "quasar": "^1.0.0-rc.2",
    "stylint": "^1.5.9",
    "uuid": "^3.3.2",
    "vue": "^2.6.10",
    "vue-i18n": "^8.11.2"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0-rc.4",
    "@types/express": "^4.16.1",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "eslint": "5.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.2.2",
    "link-module-alias": "^1.2.0",
    "nodemon": "^1.18.10",
    "strip-ansi": "=3.0.1",
    "ts-node-dev": "^1.0.0-pre.39",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "ajv": "^6.9.1"
  },
  "_moduleAliases": {
    "api": "./api",
    "root": "."
  }
}
