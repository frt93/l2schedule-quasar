{
  "name": "first",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "l2Schedule2",
  "cordovaId": "org.cordova.quasar.app",
  "author": "frt <ectoprime@gmail.com>",
  "private": true,
  "scripts": {
    "quasar": "quasar dev --mode ssr",
    "api": "nodemon api/server.js",
    "dev": "concurrently --kill-others \"npm run quasar\" \"npm run express\"",
    "docker": "docker-compose up -d",
    "build": "quasar build --mode ssr",
    "test": "echo \"No test specified\" && exit 0",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "postinstall": "link-module-alias",
    "db-backup": "docker-compose exec postgres pg_dumpall -c -U postgres > dump.sql",
    "db-restore": "docker-compose exec postgres psql -f dump.sql"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "browser-cookies": "^1.2.0",
    "cookie": "^0.3.1",
    "cookies": "^0.7.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "emailjs": "^2.2.0",
    "express": "^4.16.4",
    "graphql-request": "^1.8.2",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "lowdb": "^1.0.0",
    "mysql2": "^1.6.5",
    "pg": "^7.11.0",
    "quasar": "^1.0.0-beta.0",
    "sequelize": "^5.8.2",
    "sequelize-cli": "^5.4.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0-beta.0",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^4.0.0",
    "link-module-alias": "^1.2.0",
    "nodemon": "^1.18.10",
    "strip-ansi": "=3.0.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "ajv": "6.8.1"
  },
  "_moduleAliases": {
    "~api": "./api",
    "~root": "."
  }
}
